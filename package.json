  "repository": {
    "type": "git",
    "url": "git+https://github.com/Patrick-W-McMahon/YouTubeOfflineServer.git"
  },
  "main": "bin/www/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack-dev.config.js --content-base client/ --progress --inline --colors",
    "build": "NODE_ENV=production npm run clean && webpack --config webpack-production.config.js -p && npm run copy"
  },
  "keywords": [
